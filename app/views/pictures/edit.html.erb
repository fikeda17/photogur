<h1>Edit a Picture</h1>
<%= form_for @picture do |f| %>
  <p>
	<%= f.label :artist %><br>
	<%= f.text_field :artists %>
  </p>

  <p>
	<%= f.label :title %><br>
	<%= f.text_field :title %>
  </p>

  <p>
	<%= f.label :url%><br>
	<%= f.text_field :url %>
  </p>

  <p>
	<%= f.submit %>
  </p>
<% end %>

<p>
  <%= link_to "Back", @picture %>
</p>
